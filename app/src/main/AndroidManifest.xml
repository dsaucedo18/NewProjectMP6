<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.danny_mac.finalprojectcs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/androidsoccerapplogo"
        android:label="Android Soccer"
        android:roundIcon="@drawable/androidsoccerapplogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".myLeagues.PremierLeagueScreen" />
        <activity android:name=".myLeagues.BundesligaScreen" />
        <activity android:name=".myLeagues.SerieAScreen"></activity>
    </application>

</manifest>